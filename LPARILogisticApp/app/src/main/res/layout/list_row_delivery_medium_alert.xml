    <ImageView
        android:id="@+id/ivPhoneIcon"
        android:layout_width="13dp"
        android:layout_height="13dp"
        android:layout_alignLeft="@id/tvCustomerAddress"
        android:layout_below="@id/tvCustomerAddress"
        android:layout_marginLeft="1dp"
        android:background="@drawable/phoneicon"
        android:paddingTop="4dp"
        android:layout_marginTop="4dp"/>

    <TextView
        android:id="@+id/tvPhoneNo1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@id/ivPhoneIcon"
        android:layout_marginLeft="4dp"
        android:layout_toRightOf="@id/ivPhoneIcon"
        android:text="(669) 226-9988"
        android:textColor="#0099CC"
        android:textStyle="bold"
        android:paddingBottom="10dp"/>

<!--    <include
