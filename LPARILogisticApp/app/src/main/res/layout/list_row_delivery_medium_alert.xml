        android:layout_toRightOf="@id/tvPhoneNo1" />

    <TextView
        android:id="@+id/tvPhoneNo2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@id/tvPhoneNo1"
        android:layout_marginLeft="10dp"
        android:layout_toRightOf="@id/tvPhoneNo1"
        android:text="(408)523 6655"
        android:textColor="#0099CC" />-->

    <ImageView
        android:id="@+id/ivArrow"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_alignParentRight="true"
        android:layout_marginTop="-5dp"
        android:layout_alignTop="@id/tvCustomerName"
        android:layout_marginRight="20dp"
        android:layout_marginLeft="10dp"
        android:background="@drawable/photo_arrow" />

    <TextView
        android:id="@+id/tvNotify"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/ivArrow"
        android:layout_alignLeft="@id/ivArrow"
        android:paddingTop="2dp"
        android:layout_marginLeft="5dp"
        android:text=" Notify "
        android:textColor="#868686"
        android:textSize="12sp"
        android:textStyle="bold"/>

    <include
        layout="@layout/vertical_line"
        android:layout_width="1dip"
        android:layout_height="50dp"
        android:layout_alignTop="@id/tvCustomerName"
        android:layout_marginLeft="4dp"
        android:layout_marginRight="15dp"
        android:layout_toLeftOf="@id/ivArrow" />

