<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/rlRow"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:paddingBottom="4dp"
    android:layout_margin="5dp"
    android:background="#44FFE0B2">

    <ImageView
        android:id="@+id/ivDotIcon"
        android:layout_width="15dp"
        android:layout_height="30dp"
        android:layout_marginRight="6dp"
        android:layout_marginLeft="4dp"
        android:layout_marginTop="48dp"
        android:background="@drawable/dotscut" />


    <TextView
        android:id="@+id/tvDropNo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:layout_toRightOf="@id/ivDotIcon"
        android:padding="2dp"
        android:paddingLeft="1dp"
        android:layout_marginRight="5dp"
        android:text="Drop 1"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textColor="#33CC33" />

    <include
        layout="@layout/vertical_line"
        android:layout_width="2dip"
        android:layout_height="16dp"
        android:layout_alignTop="@id/tvDropNo"
        android:layout_marginLeft="4dp"
        android:layout_marginTop="6dp"
        android:layout_toRightOf="@id/tvDropNo" />

    <TextView
        android:id="@+id/tvDropTime"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="15dp"
        android:layout_marginTop="10dp"
        android:layout_toRightOf="@id/tvDropNo"
        android:padding="2dp"
        android:text="7:00 - 9:00 AM"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textColor="#33CC33" />

    <!--<include
        layout="@layout/vertical_line"
        android:layout_width="2dip"
        android:layout_height="15dp"
        android:layout_alignTop="@id/tvDropTime"
        android:layout_marginLeft="4dp"
        android:layout_marginTop="5dp"
        android:layout_toRightOf="@id/tvDropTime" />



    <TextView
        android:id="@+id/tvToteCount"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="5dp"
        android:layout_toRightOf="@id/tvDropTime"
        android:padding="2dp"
        android:text="4 Pkg"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textColor="#33CC33" />-->


    <TextView
        android:id="@+id/tvCustomerName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@id/tvDropNo"
        android:layout_below="@id/tvDropNo"
        android:layout_marginLeft="3dp"
        android:text="Debra Baker Barille"
        android:textColor="@color/blue_heading"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textStyle="bold"/>

    <TextView
        android:id="@+id/tvCustomerAddress"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@id/tvCustomerName"
        android:layout_below="@id/tvCustomerName"
        android:layout_marginLeft="1dp"
        android:textColor="@color/black"
        android:text="1 Washington Sq, San Jose, CA 95192"
        android:textSize="13sp" />

   <!-- <TextView
